@model ProductListModel

<div class="container">
    <div class="row">
        <div class="col-md-9">
            <partial name="_slider" />

            <div class="row">
                @foreach (var product in Model.Products)
                {
                    <partial name="_product" for="@product" />
                }
            </div>

        </div>
    </div>
</div>


